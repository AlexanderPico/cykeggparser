package org.cytoscape.keggparser.actions;

import org.cytoscape.keggparser.KEGGParserPlugin;
import org.cytoscape.keggparser.KEGGParserPluginTest;
import org.cytoscape.keggparser.parsing.KGMLConverter;
import org.junit.Before;
import org.junit.Test;
import org.osgi.framework.BundleContext;

import static junit.framework.Assert.assertNotNull;

public class KeggSaveAsBioPAXActionTest {

    private BundleContext bundleContext;
    private KEGGParserPluginTest keggParserPluginTest;
    private KEGGParserPlugin keggParserPlugin;
    private KeggSaveAsBioPAXAction keggSaveAsBioPAXActionL2;
    private KeggSaveAsBioPAXAction keggSaveAsBioPAXActionL3;

    @Before
    public void setUp() {
        keggParserPluginTest = new KEGGParserPluginTest();
        keggParserPluginTest.setUp();
        bundleContext = KEGGParserPluginTest.bundleContext;
        keggSaveAsBioPAXActionL2 = new KeggSaveAsBioPAXAction(KGMLConverter.BioPAX2, KGMLConverter.BioPAX_level2);
        keggSaveAsBioPAXActionL3 = new KeggSaveAsBioPAXAction(KGMLConverter.BioPAX3, KGMLConverter.BioPAX_level3);
    }

    @Test
    public void testConstructor(){
        KeggSaveAsBioPAXAction testKeggSaveAsBioPAXAction;
        testKeggSaveAsBioPAXAction = new KeggSaveAsBioPAXAction(0, "");
        assertNotNull(testKeggSaveAsBioPAXAction);
        assertNotNull(testKeggSaveAsBioPAXAction.getBioPaxLevel());
        assertNotNull(testKeggSaveAsBioPAXAction.getMenuName());
        assertNotNull(testKeggSaveAsBioPAXAction.getSuffix());

        testKeggSaveAsBioPAXAction = new KeggSaveAsBioPAXAction(-1, "");
        assertNotNull(testKeggSaveAsBioPAXAction);
        assertNotNull(testKeggSaveAsBioPAXAction.getBioPaxLevel());
        assertNotNull(testKeggSaveAsBioPAXAction.getMenuName());
        assertNotNull(testKeggSaveAsBioPAXAction.getSuffix());

        testKeggSaveAsBioPAXAction = new KeggSaveAsBioPAXAction(0, null);
        assertNotNull(testKeggSaveAsBioPAXAction);
        assertNotNull(testKeggSaveAsBioPAXAction.getBioPaxLevel());
        assertNotNull(testKeggSaveAsBioPAXAction.getMenuName());
        assertNotNull(testKeggSaveAsBioPAXAction.getSuffix());
    }

}
